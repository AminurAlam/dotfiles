{
  "local": { "prefix": "local", "body": "set -l " },
  "function": {
    "prefix": "function",
    "body": "function ${1:name}\n\t${2}\nend${0}"
  },
  "if": { "prefix": "if", "body": "if ${1:cond}\n\t ${2:cmd}\nend" },
  "elseif": {
    "prefix": "elseif",
    "body": "else if ${1:cond}\n\t${2:cmd}"
  },
  "else": { "prefix": "else", "body": "else\n\t${1:cmd}" },
  "switch": {
    "prefix": "switch",
    "body": "switch ${1:value}\n\tcase ${2:glob}\n\t\t${3:cmd}\n\tcase \"*\"\n\t\t${4:cmd}\nend${0}"
  },
  "for": {
    "prefix": "for",
    "body": "for ${1:i} in ${2:iterator}\n\t${3:cmd}\nend${0}"
  },
  "begin": { "prefix": "begin", "body": ["begin", "    ${1}", "end${0}"] },
  "cp": { "prefix": "cp", "body": "cp -ivr" },
  "mv": { "prefix": "mv", "body": "mv -iv" },
  "rm": { "prefix": "rm", "body": "rm -i" },
  "rf": { "prefix": "rf", "body": "rm -frI" },
  "rd": { "prefix": "rd", "body": "rmdir -pv" },
  "md": { "prefix": "md", "body": "mkdir -pv" },
  "vi": { "prefix": "vi", "body": "nvim" },
  "cls": { "prefix": "cls", "body": "clear" },
  "tar": { "prefix": "tar", "body": "tar xzf" },
  "yq": { "prefix": "yq", "body": "yq -oj --xml-attribute-prefix ''" },
  "qmv": { "prefix": "qmv", "body": "qmv -AXf do" },
  "diff": { "prefix": "diff", "body": "diff -Naur" },
  "py": { "prefix": "py", "body": "python3 -q" },
  "pst": { "prefix": "pst", "body": "ps -faxo 'pid,comm' | sed -E \"s:$PREFIX/[a-z]+/::\"" },
  "fstack": { "prefix": "fstack", "body": "ffmpeg -y -hide_banner -i ${1} -filter_complex hstack out.png" },
  "ff": { "prefix": "ff", "body": "ffmpeg -y -hide_banner -stats -loglevel error -i ${1} -vcodec copy -acodec copy -map 0:a" },
  "mbz": { "prefix": "mbz", "body": "python ~/repos/musicbrainzpy/cover_art.py -o $XDG_MUSIC_DIR/#meta/ '${1}'" },
  "awk": { "prefix": "awk", "body": "awk -F ' ' '{print $${1}}'" },
  "=": { "prefix": "=", "body": "command" },
  "...": { "prefix": "...", "body": "cd ../.." },
  "....": { "prefix": "....", "body": "cd ../../.." },
  "gac": { "prefix": "gac", "body": "git add ${1} && git commit" },
  "ga": { "prefix": "ga", "body": "git add" },
  "gc": { "prefix": "gc", "body": "git commit" },
  "gl": { "prefix": "gl", "body": "git status -bs; git log --pretty=nice -n10" },
  "gd": { "prefix": "gd", "body": "git diff" },
  "pull": { "prefix": "pull", "body": "git pull origin" },
  "push": { "prefix": "push", "body": "git push origin" },
  "fr": { "prefix": "fr", "body": "git fetch upstream && git rebase upstream/(git symbolic-ref refs/remotes/origin/HEAD | sed s@^refs/remotes/origin/@@)" },
  "rcp": { "prefix": "rcp", "body": "rclone copy -P --transfers 8 --" },
  "rls": { "prefix": "rls", "body": "rclone lsf" },
  "rlt": { "prefix": "rlt", "body": "rclone tree --level" },
  "rdu": { "prefix": "rdu", "body": "rclone size" },
  "rconf": { "prefix": "rconf", "body": "rclone config" },
  "du": { "prefix": "du", "body": "dust -Dn 25" },
  "dud": { "prefix": "dud", "body": "dust -d 1" },
  "df": { "prefix": "df", "body": "df -hx tmpfs" },
  "pi": { "prefix": "pi", "body": "yay -S" },
  "pr": { "prefix": "pr", "body": "yay -Rs" },
  "pu": { "prefix": "pu", "body": "yay -Syu" },
  "pf": { "prefix": "pf", "body": "yay -Ss" },
  "pa": { "prefix": "pa", "body": "yay -Si" },
  "zdl": { "prefix": "zdl", "body": "z $XDG_DOWNLOAD_DIR/${1}" },
  "zd": { "prefix": "zd", "body": "z $XDG_PROJECTS_DIR/dotfiles/${1}" },
  "zf": { "prefix": "zf", "body": "z $XDG_PROJECTS_DIR/dotfiles/fish/${1}" },
  "zn": { "prefix": "zn", "body": "z $XDG_PROJECTS_DIR/dotfiles/nvim/${1}" },
  "zz": { "prefix": "zz", "body": "z -" }
}
