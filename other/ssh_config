Host brick slab paper tmux
    User user
    Port 8022
    IdentityFile ${HOME}/.ssh/ssh_ed25519
    IdentitiesOnly yes
    CheckHostIP no
    StrictHostKeyChecking no

    HashKnownHosts no
    ForwardX11 no
    AddressFamily inet

Host brick
    Hostname android.local

Host tmux
    Hostname android.local
    RequestTTY yes
    Compression yes
    RemoteCommand tmux new -A -s ssh

Host slab
    Hostname 192.168.0.0

Host paper
    Hostname 192.168.0.0

Host github.com
    User git
    IdentityFile ${HOME}/.ssh/github_ed25519

Host gitlab.com
    User git
    IdentityFile ${HOME}/.ssh/gitlab_ed25519

Host codeberg.org
    User git
    IdentityFile ${HOME}/.ssh/codeberg_ed25519
